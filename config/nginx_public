# blog.thomaspuppe.de

server {

    listen 80;    
    server_name blog.thomaspuppe.de;
    root /var/www/blog_thomaspuppe_de;

    access_log /var/log/nginx/blog_thomaspuppe_de.access.log;
    error_log  /var/log/nginx/blog_thomaspuppe_de.error.log;

    index index.html;
    default_type text/html;

}